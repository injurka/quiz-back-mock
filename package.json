    "pnpm": ">=7"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsnd --respawn --pretty --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "rollup --config",
    "format": "prettier -c \"./**/*.{ts,tsx,json,md}\"",
    "format:fix": "prettier -w \"./**/*.{ts,tsx,json,md}\""
