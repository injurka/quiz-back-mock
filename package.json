    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-styles": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
